enum Duration:byte {
  FOREVER = 0,
  ONCE = 1,
  REPEATING = 2
}

enum Currency:byte {
  DOLLAR = 0,
  EURO = 1
}


table Coupon {
  id:string;
  object:string;
  livemode:bool;
  created:long;
  duration:Duration;
  metadata:[byte];
  timesRedeemed:int;
  valid:bool;
  amountOff:int;
  currency:Currency;
  durationInMonths:int;
  maxRedemptions:int;
  percentOff:int;
  redeemBy:int;
}

root_type Coupon;
